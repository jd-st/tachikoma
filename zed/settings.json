{
  "buffer_font_family": "SF Mono Powerline",
  "buffer_font_size": 15,
  "buffer_line_height": {
    "custom": 1.4
  },
  "ui_font_family": "Cantarell",
  "ui_font_size": 15,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Tachikoma"
  },
  "format_on_save": "on",
  "formatter": "language_server",
  "indent_guides": {
    "enabled": false
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "preview_tabs": {
    "enabled": false
  },
  "project_panel": {
    "file_icons": true,
    "folder_icons": false,
    "default_width": 300,
    "dock": "left",
    "indent_guides": {
      "show": "never"
    },
    "entry_spacing": "standard",
    "auto_reveal_entries": true,
    "auto_fold_dirs": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/.DS_Store",
    "**/.terraform"
  ],
  "outline_panel": {
    "file_icons": false,
    "folder_icons": false,
    "dock": "left",
    "default_width": 300,
    "indent_guides": {
      "show": "never"
    }
  },
  "terminal": {
    "dock": "bottom",
    "font_family": "FiraCode Nerd Font Mono",
    "font_size": 16,
    "font_weight": 100,
    "font_features": null,
    "line_height": "standard",
    "blinking": "on",
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "languages": {
    "Python": {
      "enable_language_server": false
    }
  },
  "lsp": {},
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "agent": {
    "enabled": false
  }
}
